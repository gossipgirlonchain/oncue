{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "countdown.html",
      "use": "@vercel/static"
    },
    {
      "src": "quiz.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.png",
      "use": "@vercel/static"
    },
    {
      "src": "api/register.js",
      "use": "@vercel/node"
    }
  ],
  "redirects": [
    {
      "source": "/quiz",
      "destination": "https://cue.market",
      "permanent": true,
      "statusCode": 301
    },
    {
      "source": "/quiz/:path*",
      "destination": "https://cue.market/:path*",
      "permanent": true,
      "statusCode": 301
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/countdown.html",
      "dest": "/countdown.html"
    },
    {
      "src": "/quiz.html",
      "dest": "/quiz.html"
    },
    {
      "src": "/(.*\\.png)",
      "dest": "/$1"
    },
    {
      "src": "/api/register",
      "dest": "/api/register.js"
    }
  ]
}
